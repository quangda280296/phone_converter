<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.noname.quangcaoads"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions"/>

    <application>
        <!--<activity-->
            <!--android:name="co"-->
            <!--android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"-->
            <!--android:excludeFromRecents="true"-->
            <!--android:icon="@drawable/ic_null_stubload_xxxads"-->
            <!--android:label=""-->
            <!--android:noHistory="true"-->
            <!--android:theme="@style/Theme.Transparent"/>-->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"
            android:theme="@android:style/Theme.Translucent"/>

        <activity
            android:name=".AdsActivityFull"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"
            android:theme="@style/Theme.TransparentAds"/>

        <activity
            android:name="com.facebook.ads.AudienceNetworkActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"/>

        <activity
            android:name="com.startapp.android.publish.ads.list3d.List3DActivity"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"
            android:theme="@android:style/Theme"/>

        <activity
            android:name="com.startapp.android.publish.adsCommon.activities.OverlayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"
            android:theme="@android:style/Theme.Translucent"/>

        <activity
            android:name="com.startapp.android.publish.adsCommon.activities.FullScreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:icon="@drawable/ic_null_stubload_xxxads"
            android:label=""
            android:noHistory="true"
            android:theme="@android:style/Theme"/>

        <receiver android:name="com.noname.quangcaoads.UserPresentReceiver">
            <intent-filter
                android:enabled="true"
                android:exported="false">
                <action android:name="android.intent.action.USER_PRESENT"/>
            </intent-filter>
        </receiver>

        <receiver android:name="com.noname.quangcaoads.ComVsAppReceiver">
            <intent-filter>
                <action android:name="com.nnvd.comvsapp"/>
            </intent-filter>
        </receiver>

        <service android:name="net.mready.hover.HoverService"/>

        <service
            android:name="com.noname.quangcaoads.AppCheckServices"
            android:enabled="true"/>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <service
            android:name=".JobServiceAndroidO"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name=".LockService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" >
        </service>

        <receiver android:name="com.noname.quangcaoads.LockService$LockReceiver">
            <intent-filter>
                <action android:name="android.intent.action.SCREEN_OFF"/>
                <action android:name="android.intent.action.SCREEN_ON"/>
            </intent-filter>
        </receiver>

        <receiver android:name="com.noname.quangcaoads.JobServiceAndroidO$AdsReceiver">
            <intent-filter>
                <action android:name="android.intent.action.SCREEN_OFF"/>
                <action android:name="android.intent.action.SCREEN_ON"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>
